KAAI,CAAC,SAAL,GAAiB,QAAQ,CAAC,IAA1B;AACD;AACF,SAND;AAOD;AACJ,KAtBI;AAuBP,IAAA,YAvBO,0BAuBK;AACV,UAAI,KAAK,QAAL,CAAc,MAAd,IAAwB,CAA5B,EAA+B,KAAK,WAAL;AAC/B,WAAK,gBAAL,GAAwB,IAAxB;AACD,KA1BM;AA2BP,IAAA,YA3BO,0BA2BK;AAAA;;AACV,UAAI,CAAC,KAAK,eAAV,EAA2B;AAC3B,MAAA,GAAG,CAAC,OAAJ,CAAY,MAAZ,CAAmB,KAAK,iBAAxB,EAA2C,KAAK,IAAhD,EAAsD,IAAtD,CAA2D,UAAA,QAAQ,EAAG;AACpE,gBAAQ,QAAQ,CAAC,MAAjB;AACE,eAAK,GAAL;AACE,YAAA,MAAI,CAAC,QAAL,CAAc,IAAd,CAAmB,QAAQ,CAAC,IAA5B;;AACA,YAAA,MAAI,CAAC,iBAAL,GAAyB,EAAzB;AACA;AAJJ;AAMD,OAPD;AAQD,KArCM;AAsCP,IAAA,WAtCO,yBAsCI;AAAA;;AACT,UAAI,KAAK,iBAAL,IAA0B,CAAC,CAA/B,EAAkC;AAClC,WAAK,eAAL,GAAuB,IAAvB;AACA,WAAK,gBAAL,CAAsB,IAAtB;AACA,MAAA,GAAG,CAAC,IAAJ,CAAS,QAAT,CAAkB,KAAK,IAAvB,EAA6B,KAAK,iBAAlC,EAAqD,IAArD,CAA0D,UAAA,QAAQ,EAAG;AAAA;;AACnE,gBAAQ,QAAQ,CAAC,MAAjB;AACE,eAAK,GAAL;AACE,+BAAA,MAAI,CAAC,QAAL,EAAc,IAAd,2CAAsB,QAAQ,CAAC,IAAT,CAAc,IAApC;;AAEA,gBAAI,MAAI,CAAC,iBAAL,IAA0B,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,SAAjD,EAA4D,MAAI,CAAC,iBAAL,GAAyB,CAAC,CAA1B,CAA5D,KACK,MAAI,CAAC,iBAAL;AAEL,YAAA,MAAI,CAAC,eAAL,GAAuB,KAAvB;;AACA,YAAA,MAAI,CAAC,gBAAL,CAAsB,IAAtB;;AACA;;AACF;AACE;AAXJ;AAaD,OAdD;AAeD;AAzDM;AA3DkB,CAAD,CAA9B","sourcesContent":["\nimport {defineComponent, PropType} from 'vue'\nimport vClickOutside from '../../click-outside'\nimport moment from 'moment';\nimport {mapGetters} from 'vuex'\nimport {API} from '@/api';\nimport lottie, {AnimationItem} from 'lottie-web';\nimport {Comment, Post} from '@/models'\nimport TagLabel from '../utils/TagLabel.vue'\nimport CommentCard from \"@/components/cards/CommentCard.vue\";\nimport TextArea from \"@/components/inputs/TextArea.vue\";\n\nexport default defineComponent({\n  components: {\n    TagLabel,\n    TextArea,\n    CommentCard\n  },\n  directives: {\n    clickOutside: vClickOutside\n  },\n    data() {\n      return {\n        post_data: this.post as Post,\n        likeAnimation: undefined as unknown as AnimationItem,\n        loadingAnimation: undefined as unknown as AnimationItem,\n        loadingComments: false,\n        next_comment_page: 1,\n        display_comments: true,\n        comments: new Array<Comment>(),\n        newCommentContent: \"\"\n      }\n    },\n    props: {\n        post: {\n            type: Object as PropType<Post>,\n            required: true,\n            default: () => {return null;}\n        },\n    },\n    mounted() {\n\n      this.likeAnimation = lottie.loadAnimation({\n        container: this.$refs['likeIcon'] as Element,\n        renderer: 'svg',\n        loop: false,\n        autoplay: false,\n        path: '/lottie/heart.json'\n      });\n\n      this.loadingAnimation = lottie.loadAnimation({\n        container: this.$refs['loader'] as Element,\n        renderer: 'svg',\n        loop: true,\n        autoplay: false,\n        path: '/lottie/loading.json'\n      });\n\n      if (this.post_data.liked) {\n        this.likeAnimation.play();\n      }\n    },\n    computed: {\n        timeSinceCreation(): string {\n          return moment((this.post_data as Post).created_at).fromNow();\n        },\n        ...mapGetters({\n            user: 'user',\n            isAuthenticated : 'isAuthenticated'\n        })\n    },\n    methods: {\n        like: function(like: boolean) {\n            if (!this.isAuthenticated) return;\n            if (like) {\n\n              API.Post.like(this.post).then(response => {\n                if (response.status == 200) {\n                  this.likeAnimation.setDirection(1);\n                  this.likeAnimation.play();\n                  this.post_data = response.data as Post;\n                }\n              })\n            } else {\n\n              API.Post.unlike(this.post).then(response => {\n                if (response.status == 200) {\n                  this.likeAnimation.setDirection(-1);\n                  this.likeAnimation.play();\n                  this.post_data = response.data as Post;\n                }\n              });\n            }\n        },\n      showComments() {\n        if (this.comments.length == 0) this.addComments();\n        this.display_comments = true;\n      },\n      writeComment() {\n        if (!this.isAuthenticated) return;\n        API.Comment.create(this.newCommentContent, this.post).then(response => {\n          switch (response.status) {\n            case 201:\n              this.comments.push(response.data);\n              this.newCommentContent = \"\";\n              break;\n          }\n        });\n      },\n      addComments() {\n        if (this.next_comment_page == -1) return;\n        this.loadingComments = true;\n        this.loadingAnimation.play();\n        API.Post.comments(this.post, this.next_comment_page).then(response => {\n          switch (response.status) {\n            case 200:\n              this.comments.push(...response.data.data);\n\n              if (this.next_comment_page == response.data.meta.last_page) this.next_comment_page = -1;\n              else this.next_comment_page++;\n\n              this.loadingComments = false;\n              this.loadingAnimation.stop();\n              break;\n            default:\n              break;\n          }\n        });\n      }\n    }\n})\n"],"sourceRoot":""}]}